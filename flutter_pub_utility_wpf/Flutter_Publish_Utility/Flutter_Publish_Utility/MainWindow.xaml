<Window x:Class="Flutter_Publish_Utility.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flutter_Publish_Utility"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>
        <Button Height="70" Width="200" x:Name="powerShellButton"
                Content="Execute Power Shell"
                Click="PowerShellButton_Click"/>

        <Button Height="70" Width="200" x:Name="pubPublish"
                Content="Pub Publish" Grid.Row="1"
                Click="PubPublish_Click"/>
        
        <Button Height="70" Width="200" x:Name="generateAPK"
                Content="Pub Publish" Grid.Row="2"
                Click="GenerateAPK_Click"/>

        <Button Height="70" Width="200" x:Name="webHost"
                Content="Pub Publish" Grid.Row="3"
                Click="WebHost_Click"/>
    </Grid>
</Window>
